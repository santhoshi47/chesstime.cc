import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	  {
int t,n;
Scanner s=new Scanner(System.in);
	    t=s.nextInt();
	    for(int i=0;i<t;i++)
	    {
	        int a=1;
	        n=s.nextInt();
	        for(int j=1;j<=n;j++)
	        {
	            a=a*j;
	        }
	   System.out.println(a);
	    }
	}
}
