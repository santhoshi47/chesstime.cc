
import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    int t,n,x,y;
		Scanner s= new Scanner(System.in);
		t=s.nextInt();
		{
		    for(int i=0;i<t;i++)
		    {
		        x=s.nextInt();
		        y=s.nextInt();
		    {
		if(x>y||x==y)
		System.out.println("yes");
		else if (x<y)
		System.out.println("no");
		
	    }	}}}
}
