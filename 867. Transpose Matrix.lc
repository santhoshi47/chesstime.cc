class Solution {
    public int[][] transpose(int[][] matrix) {
        int rows=matrix.length;
        int columns=matrix[0].length;
        int[][] matrix1=new int[columns][rows];
        for(int i=0;i<rows;i++)
        {
            for(int j=0;j<columns;j++)
            {
                matrix1[j][i]=matrix[i][j];
            }
        }
        return matrix1;
    }
}
