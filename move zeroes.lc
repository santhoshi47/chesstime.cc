class Solution {
    public void moveZeroes(int[] nums) {
        int insertposi=0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]!=0)
            {
                nums[insertposi]=nums[i];
                insertposi++;
            }
        }
        while(insertposi<nums.length){
            nums[insertposi++]=0;
        
    }}}
    
    
    
