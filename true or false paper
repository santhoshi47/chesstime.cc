import java.util.*;
import java.lang.*;
import java.io.*;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int t,n,k;
		Scanner s=new Scanner(System.in);
		t=s.nextInt();
		{
		    for(int i=0;i<t;i++)
		    {
		        n=s.nextInt();
		        k=s.nextInt();
		        System.out.println(n-k);
		    }
		}
	}
}
