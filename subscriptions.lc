import java.util.*;
import java.lang.*;
import java.io.*;
import java.lang.Math;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    int t,n,x;
	    double h;
		Scanner s=new Scanner(System.in);
		t=s.nextInt();

		    for(int i=0;i<t;i++){
		    n=s.nextInt();
		    x=s.nextInt();
		    h=n/6;
		    if(n%6==0)
		    System.out.println(h*x);
		    else
		    System.out.println((h+1)*x);
		}
	}
}
